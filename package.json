{
  "scripts": {
    "asbuild:untouched": "asc src/assembly/index.ts -b build/untouched.wasm -t build/untouched.wat --validate --sourceMap --debug",
    "asbuild:optimized": "asc src/assembly/index.ts -b build/optimized.wasm -t build/optimized.wat --validate --sourceMap -O3 --noAssert",
    "asbuild": "npm run asbuild:untouched && npm run asbuild:optimized",
    "spbuild": "wat2wasm src/native/spectrum.wat -o build/spectrum.wasm",
    "sp": "node spectrum.js",
    "test": "ts-mocha -p ./test/tsconfig.json ./test/**/*.test.ts"
  },
  "dependencies": {},
  "devDependencies": {
    "@assemblyscript/loader": "^0.9.4",
    "@types/expect": "^24.3.0",
    "@types/mocha": "^7.0.2",
    "@types/node": "^13.9.1",
    "assemblyscript": "^0.9.4",
    "expect": "^25.1.0",
    "mocha": "^7.1.1",
    "ts-mocha": "^6.0.0",
    "typescript": "^3.8.3",
    "wat2wasm": "^1.0.2"
  }
}
